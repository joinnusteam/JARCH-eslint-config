{
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "printWidth": 120,
  "endOfLine": "lf",
  "organizeImportsSkipDestructiveCodeActions": true,
  "plugins": ["prettier-plugin-pkg", "prettier-plugin-organize-imports", "prettier-plugin-sh"]
}
